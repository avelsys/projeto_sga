pipeline{
     agent any
     stages{
       stage('build'){
         steps{
            echo 'Fazendo Build'
         }
      }
       stage('Compilação'){
         steps{
           echo 'Fazendo compilação'
         }
      }
       stage('Testes'){
         steps{
            echo 'reakuzando testes'
         }
      }      
   }
}   
