pipeline{
     agent any
     stages{
       stage('build'){
         steps{
           cmd_exec('echo "Buils starting..."')
           cmd_exec('echo "dir /a /b"')            
         }
      }
       stage('Compilação'){
         steps{
           echo 'Fazendo compilação'
         }
      }
       stage('Testes'){
         steps{
            echo 'reakuzando testes'
         }
      }      
   }
}   
